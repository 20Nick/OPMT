<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Scarecrow.games.OPMT">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconcircle"
        android:supportsRtl="true"
        android:theme="@style/Theme.MuTorere">
        <activity android:name="com.Scarecrow.games.OPMT.GameActivity" android:configChanges="orientation" android:screenOrientation="portrait"></activity>
        <activity android:name="com.Scarecrow.games.OPMT.MainMenu" android:configChanges="orientation" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>